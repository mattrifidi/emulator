package org.rifidi.emulator.reader.control.adapter;

/**
 *
 *
 * @author Matthew Dean
 * @since  <$INITIAL_VERSION$>
 * @version <$CURRENT_VERSION$>
 */
public class CommandNotFoundException extends Exception {

	/**
	 * The serial version ID for this class.
	 */
	private static final long serialVersionUID = 7639628657185622052L;
	
	/**
	 * An exception that occurs because the current command given is not found.  
	 * 
	 * @param arg0
	 * 				The message that is passed in.  
	 */
	public CommandNotFoundException(String arg0) {
		super(arg0);
	}
}
