<reader>
	<readerclassname>org.rifidi.emulator.reader.awid.module.AwidReaderModule</readerclassname>
	<description>Emulator for the Awid MPR</description>
	<maxantennas>2</maxantennas>
	<maxgpis>0</maxgpis>
	<maxgpos>0</maxgpos>
	<property>
		<required>1</required>
		<name>inet_address</name>
		<display>ip address</display>
		<tooltip>The address the reader should be reachable through</tooltip>
		<defaultvalue>127.0.0.1:4000</defaultvalue>		
		<validatorclassname>IpAddressValidator</validatorclassname>
	</property>
</reader>