package org.rifidi.ui.common.reader.callback;

/**
 * @author Andreas Huebner - andreas@pramari.com
 * 
 */
public interface GPOEventCallbackInterface {

	/**
	 * this is the callback method beeing invoked when a GPO port was set to
	 * high
	 * 
	 * @param gpoPortNum
	 */
	public void GPOPortSetHigh(int gpoPortNum);

	/**
	 * this is the callback method beeing invoked when a GPO port was set to low
	 * 
	 * @param gpoPortNum
	 */
	public void GPOPortSetLow(int gpoPortNum);
}
