<reader>
	<readerclassname>org.rifidi.emulator.reader.symbol.module.SymbolReaderModule</readerclassname>
	<description>Emulator for the Symbol XR400</description>
	<maxantennas>4</maxantennas>
	<property>
		<required>1</required>
		<name>byte_address</name>
		<display>byte ip address</display>
		<tooltip>The address the reader should be reachable through for the 
		byte encoding</tooltip>
		<defaultvalue>127.0.0.1:3000</defaultvalue>		
		<validatorclassname>IpAddressValidator</validatorclassname>
	</property>
	<property>
		<required>1</required>
		<name>http_address</name>
		<display>ip address for http</display>
		<tooltip>The address the reader should be reachable through</tooltip>
		<defaultvalue>127.0.0.1:10080</defaultvalue>		
		<validatorclassname>IpAddressValidator</validatorclassname>
	</property>
</reader>