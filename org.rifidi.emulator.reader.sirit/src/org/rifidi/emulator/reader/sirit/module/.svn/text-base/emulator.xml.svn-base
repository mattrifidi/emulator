<reader>
	<readerclassname>org.rifidi.emulator.reader.sirit.module.SiritReaderModule</readerclassname>
	<description>Emulator for the Sirit INfinity 510</description>
	<maxantennas>4</maxantennas>
	<maxgpis>0</maxgpis>
	<maxgpos>0</maxgpos>
	<property>
		<required>1</required>
		<name>ipaddress</name>
		<display>ip address</display>
		<tooltip>The address the reader should be reachable through</tooltip>
		<defaultvalue>127.0.0.1</defaultvalue>		
		<validatorclassname>ReaderNameValidator</validatorclassname>
	</property>
	<property>
		<required>1</required>
		<name>communicationport</name>
		<display>communication port</display>
		<tooltip>The port on which the client communicates with the reader</tooltip>
		<defaultvalue>50007</defaultvalue>		
		<validatorclassname>PositiveIntegerValidator</validatorclassname>
	</property>
	<property>
		<required>1</required>
		<name>eventport</name>
		<display>event port</display>
		<tooltip>The port on which the readersends its event messages</tooltip>
		<defaultvalue>50008</defaultvalue>		
		<validatorclassname>PositiveIntegerValidator</validatorclassname>
	</property>
</reader>