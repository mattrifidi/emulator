<reader>
	<readerclassname>org.rifidi.emulator.reader.llrp.module.LLRPReaderModule</readerclassname>
	<description>For an explanation of the server mode feature, see the LLRPHelloWorld QuickStart Guide at http://www.rifidi.org/llrp/</description>
	<maxantennas>4</maxantennas>
	<maxgpis>4</maxgpis>
	<maxgpos>4</maxgpos>
	<property>
		<required>1</required>
		<name>inet_address</name>
		<display>LLRP Administration IP Address</display>
		<tooltip>The address the reader should be reachable through</tooltip>
		<defaultvalue>127.0.0.1:10101</defaultvalue>		
		<validatorclassname>IpAddressValidator</validatorclassname>
	</property>
	<property>
		<required>1</required>
		<name>servermode</name>
		<display>Enable Server Mode at startup</display>
		<tooltip>Startup Reader in ServerMode</tooltip>
		<defaultvalue>1</defaultvalue>
		<validatorclassname>BooleanValidator</validatorclassname>
	</property>	
	<property>
		<required>1</required>
		<name>llrp_inet_address</name>
		<display>LLRP Connection IP Address</display>
		<tooltip>The address the reader should set up for incoming LLRP Connections</tooltip>
		<defaultvalue>127.0.0.1:5084</defaultvalue>		
		<validatorclassname>IpAddressValidator</validatorclassname>
	</property>
</reader>